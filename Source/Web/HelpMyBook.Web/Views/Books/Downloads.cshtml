@model ICollection<HelpMyBook.Web.ViewModels.Books.BookViewModel>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}


@if (Model.Count <= 0)
{
    <h1>There are no Downloads!</h1>
}
else
{
    foreach (var book in Model)
    {
        <h1>Id: @book.Id</h1>

            <h3>i'm in</h3>
            @Html.ActionLink($"Go to details", "Details", "Books",new { id = book.Id } ,new { @class = "btn btn-primary"});
        <hr />
    }
}

